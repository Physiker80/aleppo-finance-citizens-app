{
  "name": "aleppo-directorate-of-finance---complaints-and-inquiries-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "server:csp-enforce": "cross-env CSP_ENABLED=true CSP_REPORT_ONLY=false PORT=4000 node server.js",
    "server:csp-enforce:port": "cross-env CSP_ENABLED=true CSP_REPORT_ONLY=false node server.js",
    "db:init": "prisma migrate dev --name init",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "test:prepare": "powershell -Command \"if (Test-Path dev-test.db) { Remove-Item dev-test.db -Force }\" && cross-env DATABASE_URL=file:./dev-test.db prisma db push --skip-generate",
    "test": "npm run test:prepare && cross-env DATABASE_URL=file:./dev-test.db vitest run --",
    "restore:create": "node scripts/create-restore-point.js",
    "backup": "node scripts/create-restore-point.js",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:sync:android": "npx cap sync android",
    "cap:sync:ios": "npx cap sync ios",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "android:dev": "npm run build && npx cap sync android && npx cap run android -l --external",
    "android:dev:external": "npm run build && npx cap sync android && npx cap run android -l --external",
    "ios:open": "npx cap open ios",
    "ios:run": "npx cap run ios",
    "ios:dev": "npm run build && npx cap sync ios && npx cap run ios -l --external",
    "ios:dev:external": "npm run build && npx cap sync ios && npx cap run ios -l --external",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:dev": "npm run build && npx cap sync && echo 'Use android:run or ios:run to launch'",
    "cap:sync:dev": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap sync",
    "cap:sync:dev:android": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap sync android",
    "cap:sync:dev:ios": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap sync ios",
    "android:dev:url": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap run android",
    "ios:dev:url": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap run ios",
    "android:dev:url:adb": "cross-env CAP_USE_DEV_SERVER=1 DEV_SERVER_PORT=5175 npx cap sync android && adb shell rm -f /sdcard/Android/data/com.aleppo.finance.system/files/capacitor.config.json; adb push .\\android\\app\\src\\main\\assets\\capacitor.config.json /sdcard/Android/data/com.aleppo.finance.system/files/ && npx cap run android",
    "gradle:clean": "java -cp .\\android\\gradle\\wrapper\\gradle-wrapper.jar org.gradle.wrapper.GradleWrapperMain clean -p android",
    "gradle:assembleDebug": "java -cp .\\android\\gradle\\wrapper\\gradle-wrapper.jar org.gradle.wrapper.GradleWrapperMain assembleDebug -p android",
    "gradle:assembleRelease": "java -cp .\\android\\gradle\\wrapper\\gradle-wrapper.jar org.gradle.wrapper.GradleWrapperMain assembleRelease -p android",
    "gradle:bundleRelease": "java -cp .\\android\\gradle\\wrapper\\gradle-wrapper.jar org.gradle.wrapper.GradleWrapperMain bundleRelease -p android",
    "report:docx": "node scripts/generate-docx-report.js",
    "hmac:sign": "node scripts/hmac-sign.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@capacitor/android": "^7.4.3",
    "@capacitor/camera": "^7.0.3",
    "@capacitor/haptics": "^7.0.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/preferences": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation": "^0.205.0",
    "@opentelemetry/instrumentation-document-load": "^0.50.0",
    "@opentelemetry/instrumentation-fetch": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-web": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.12.0",
    "@sentry/react": "^10.12.0",
    "@supabase/supabase-js": "^2.93.3",
    "@xyflow/react": "^12.8.4",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "argon2": "^0.44.0",
    "bcryptjs": "^2.4.3",
    "canvg": "^4.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.544.0",
    "mammoth": "^1.10.0",
    "mermaid": "^11.11.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.13",
    "otplib": "^12.0.1",
    "pdfjs-dist": "5.3.93",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-pdf": "^10.1.0",
    "react-qr-code": "^2.0.18",
    "sanitize-html": "^2.17.0",
    "svg2pdf.js": "^2.5.0",
    "tesseract.js": "^6.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "compression": "^1.8.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
