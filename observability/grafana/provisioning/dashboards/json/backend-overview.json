{
  "annotations": {"list": []},
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per second",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "RPS"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p99",
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error rate %",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m])))",
          "legendFormat": "5xx%"
        }
      ]
    }
    ,
    {
      "type": "timeseries",
      "title": "CPU Usage (process)",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(alf_process_cpu_user_seconds_total[1m]) + rate(alf_process_cpu_system_seconds_total[1m])",
          "legendFormat": "cpu user+system sec/s"
        }
      ]
    }
    ,
    {
      "type": "stat",
      "title": "Memory RSS (bytes)",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "alf_process_resident_memory_bytes",
          "legendFormat": "rss"
        }
      ],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}}
    }
  ],
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["backend"],
  "templating": {"list": []},
  "time": {"from": "now-6h", "to": "now"},
  "timepicker": {},
  "timezone": "",
  "title": "Backend Overview",
  "version": 1
}
